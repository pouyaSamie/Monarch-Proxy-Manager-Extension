{
  
    "manifest_version": 2,
    "name": "Monarch proxy Manager",
    "version": "1.0",
    "permissions": ["proxy", "storage","https://api.ipify.org/*"],
    "background": {
      "scripts": ["js/background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
  
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  }
}
